  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.9.0",
    "@types/jest": "^25.2.1",
    "cheminfo-build": "^1.1.0",
    "convert-to-jcamp": "^2.0.1",
    "eslint": "^6.8.0",
